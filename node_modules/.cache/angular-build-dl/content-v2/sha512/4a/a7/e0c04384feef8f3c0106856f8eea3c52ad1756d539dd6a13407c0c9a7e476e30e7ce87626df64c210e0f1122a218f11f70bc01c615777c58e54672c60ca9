    constructor(af, afAuth, menu, db) {
        this.af = af;
        this.afAuth = afAuth;
        this.menu = menu;
        this.db = db;
        const user = af.doc(`users/${this.db.getUID()}`);
        this.user = user.valueChanges();
    }
    ionViewWillEnter() {
        this.menu.enable(false);
    }
    ionViewDidLeave() {
        this.menu.enable(false);
    }
    loaded() {
        this.afAuth.authState.subscribe(data => console.log(data));
    }
    openFirst() {
        this.menu.enable(true, 'first');
